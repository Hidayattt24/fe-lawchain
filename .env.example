# Frontend Environment Variables Example
# Copy this file to .env and fill in the actual values

# Backend API Configuration
# URL to connect to the LawChain backend API
NEXT_PUBLIC_API_BASE_URL=http://127.0.0.1:8000/api/v1

# Environment
# Set to 'development', 'staging', or 'production'
NODE_ENV=development

# Optional: Custom port for Next.js development server
# PORT=3000

# Optional: Custom host for Next.js development server  
# HOST=localhost

# Optional: Disable telemetry
# NEXT_TELEMETRY_DISABLED=1
